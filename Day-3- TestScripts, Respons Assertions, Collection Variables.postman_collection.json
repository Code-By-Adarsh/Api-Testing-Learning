		{
			"name": "H-2",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const body = pm.response.json();\r",
							"\r",
							"pm.test(\"ID is number\",function(){\r",
							"    pm.expect(body.data.id).to.be.a(\"number\");\r",
							"});\r",
							"\r",
							"pm.test(\"Email is string\",function(){\r",
							"    pm.expect(body.data.email).to.be.a(\"string\");\r",
							"});\r",
							"\r",
							"pm.test(\"Avatar URL matched regex\",function(){\r",
							"    pm.expect(body.data.avatar).to.match(/^https?:\\/\\/.*\\.(jpg|png)$/);\r",
							"});\r",
							"\r",
							"pm.test(\"Data object has all keys\",function(){\r",
							"    pm.expect(body.data).to.have.all.keys(\"id\",\"email\",\"first_name\",\"last_name\",\"avatar\");\r",
							"});\r",
							"\r",
							"pm.test(\"Status code is 2xx\",function(){\r",
							"    pm.expect(pm.response.code).to.within(200,299);\r",
							"});\r",
							"\r",
							"pm.test(\"Response time < 500ms\",function(){\r",
							"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
							"});"
						],
